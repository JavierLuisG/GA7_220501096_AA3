create database if not exists hlf;
use hlf;
create table usuario (
	user_id int auto_increment primary key not null,
    user_email varchar(100) not null,
    user_password varchar(100) not null
);
insert into usuario (user_email, user_password) 
	values ("admin@gmail.com", "admin"),
    ('pepito@gmail.com','pepito'),('sutano@gmail.com','sutano'),
    ('fulano@gmail.com','fulano'),('elio@gmail.com','elio');